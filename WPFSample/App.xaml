                    <!-- *********************************  ToolTip Style  ********************************* -->
                    <Style x:Key="ToolTipBaseStyle" TargetType="ToolTip">
                        <Setter Property="Background" Value="{StaticResource UIUXBrush_White}"/>
                        <Setter Property="Placement" Value="Bottom"/>
                        <Setter Property="VerticalOffset" Value="10"/>
                        <Setter Property="ContentTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <TextBlock Text="{Binding}" TextWrapping="Wrap" FontFamily="Segoe UI Semibold" FontSize="11"/>
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="ToolTip">
                                    <Border Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" MaxWidth="240" BorderThickness="1">
                                        <ContentPresenter Margin="17,7,17,10" TextElement.Foreground="{TemplateBinding Foreground}"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>

                    <!-- _________________________ Error ToolTip ______________________________________ -->
                    <Style x:Key="ErrorToolTipStyle" TargetType="ToolTip" BasedOn="{StaticResource ToolTipBaseStyle}">
                        <Setter Property="Foreground" Value="{StaticResource UIUXBrush_Red}"/>
                        <Setter Property="BorderBrush" Value="{StaticResource UIUXBrush_Red}"/>
                    </Style>

                    <!-- _________________________ Warning ToolTip ______________________________________ -->
                    <Style x:Key="WarningToolTipStyle" TargetType="ToolTip" BasedOn="{StaticResource ToolTipBaseStyle}">
                        <Setter Property="Foreground" Value="{StaticResource UIUXBrush_Warning}"/>
                        <Setter Property="BorderBrush" Value="{StaticResource UIUXBrush_Warning}"/>
                    </Style>

                    <!-- _________________________ Blue ToolTip ______________________________________ -->
                    <Style x:Key="BlueToolTipStyle" TargetType="ToolTip" BasedOn="{StaticResource ToolTipBaseStyle}">
                        <Setter Property="Foreground" Value="{StaticResource UIUXBrush_ToolTip}"/>
                        <Setter Property="BorderBrush" Value="{StaticResource UIUXBrush_ToolTip}"/>
                        <Setter Property="VerticalOffset" Value="2"/>
                        <Style.Triggers>
                            <Trigger Property="Content" Value="{x:Null}">
                                <Setter Property="Visibility" Value="Collapsed"/>
                            </Trigger>
                            <Trigger Property="Content" Value="">
                                <Setter Property="Visibility" Value="Collapsed"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>

                    <!--  _________________________ Editor Validation ErrorTemplate __________________________________________  -->
                    <ControlTemplate x:Key="EditorErrorTemplate">
                        <DockPanel>
                            <Image Style="{StaticResource ErrorImageStyle}" DockPanel.Dock="Left" Margin="0,0,5,0" ToolTipService.InitialShowDelay="0">
                                <Image.ToolTip>
                                    <local:CalloutToolTip Content="{Binding Path=[0].ErrorContent}" Style="{StaticResource ErrorToolTipStyle}" PointerPosition="0.1"/>
                                </Image.ToolTip>
                            </Image>
                            <AdornedElementPlaceholder VerticalAlignment="Center">
                                <Border BorderBrush="{StaticResource UIUXBrush_Error}" BorderThickness="1" SnapsToDevicePixels="True"/>
                            </AdornedElementPlaceholder>
                        </DockPanel>
                    </ControlTemplate>

                   
                </ResourceDictionary>

